
public class YourInitials {
	public static void main(String[] args) {
		System.out.println("J J J J J");
		System.out.println("    J    ");
		System.out.println("    J    ");
		System.out.println("    J    ");
		System.out.println("J   J    ");
		System.out.println("J   J    ");
		System.out.println("J J J    ");
		
		System.out.println();
		System.out.println();
		System.out.println();
		System.out.println();
		System.out.println();
		System.out.println();
		System.out.println();
	}
}
